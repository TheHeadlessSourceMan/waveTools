    "from productionMud.curves import *\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "def plotCurve(curve:CurveBase):\n",
    "    ypoints=curve.samples()\n",
    "    xpoints=np.array(range(len(ypoints)))\n",
    "    plt.plot(xpoints,ypoints)\n",
    "\n",
    "plotCurve(GaussianCurve(5,2))\n",
    "\n",
    "plt.show()"
   ]
  }
